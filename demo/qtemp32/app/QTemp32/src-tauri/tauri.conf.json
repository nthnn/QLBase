{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "qtemp32",
  "version": "0.1.0",
  "identifier": "xyz.nathannestein.qlbase.qtemp32.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "QTemp32",
        "width": 580,
        "height": 380,
        "center": true,
        "maximizable": false,
        "minimizable": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
