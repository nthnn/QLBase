{
    "manifest_version": 3,
    "name": "QTracker",
    "version": "1.0",
    "description": "Demo browsing history tracker extension using QLBase.",
    "permissions": ["tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "QTracker"
    }
}
